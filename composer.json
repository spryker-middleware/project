{
    "name": "spryker-middleware/project",
    "description": "Spryker Middleware",
    "type": "project",
    "license": "proprietary",
    "require": {
        "ext-json": "*",
        "spryker-middleware/process": "^1.0.0",
        "spryker/console": "^4.0.0",
        "spryker/queue": "^1.6.0",
        "spryker/rabbit-mq": "^2.6.0",
        "spryker/store": "^1.11.0",
        "spryker/util-encoding": "^2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0.0",
        "spryker/architecture-sniffer": "*",
        "spryker/calculation": "*",
        "spryker/cart": "*",
        "spryker/code-sniffer": "*",
        "spryker/development": "^3.0.0",
        "spryker/propel": "*",
        "spryker/silex": "*",
        "spryker/testify": "*",
        "spryker/transfer": "^3.10.0"
    },
    "autoload": {
        "psr-0": {
            "": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600
    },
    "scripts": {
        "cs-check": "phpcs --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v src/",
        "cs-fix": "phpcbf --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v src/"
    }
}
