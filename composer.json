{
    "name": "spryker-middleware/project",
    "description": "Spryker Middleware",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "spryker-middleware/process": "dev-feature/dev"
    },
    "require-dev": {
        "php-mock/php-mock": "^1.0",
        "phploc/phploc": "^3.0.1",
        "phpmd/phpmd": "~2.4.3",
        "phpunit/phpunit": "^5.0",
        "spryker/architecture-sniffer": "dev-master",
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Middleware\\": "src/Middleware/",
            "Generated\\": "src/Generated/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 600
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://code.spryker.com/repo/private"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker/architecture-sniffer.git"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker-middleware/process.git"
        }
    ]
}
