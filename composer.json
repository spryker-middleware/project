{
    "name": "spryker-middleware/project",
    "description": "Spryker Middleware",
    "type": "project",
    "license": "MIT",
    "require": {
        "league/pipeline": "^0.3.0",
        "spryker/console": "^3.0.0",
        "spryker/development": "^3.0.0"
    },
    "require-dev": {
        "php-mock/php-mock": "^1.0",
        "phploc/phploc": "^3.0.1",
        "phpmd/phpmd": "~2.4.3",
        "phpunit/phpunit": "^5.0",
        "spryker/architecture-sniffer": "^0.3.0",
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-0": {
            "": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 600
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://code.spryker.com/repo/private"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker/architecture-sniffer.git"
        }
    ]
}
